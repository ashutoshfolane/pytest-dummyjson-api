{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "accessToken": { "type": "string" },
    "refreshToken": { "type": "string" },
    "token": { "type": "string" },
    "id": { "type": "integer" },
    "username": { "type": "string" }
  },
  "anyOf": [
    { "required": ["accessToken", "refreshToken"] },
    { "required": ["token"] }
  ],
  "additionalProperties": true
}